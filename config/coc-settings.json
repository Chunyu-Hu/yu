{
	"languageserver": {
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"rootPatterns": [".git", ".bash_lsp"],
			"filetypes": ["sh"],
			"enable": false
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
			"rootPatterns": [".ccls", "compile_commands.json"],
			"initializationOptions": {
				"cache": {
					"directory": ".ccls-cache"
				},
				"client": {
					"snippetSupport": true
				}
			},
			"enable": false
		},
	},
	"yaml.schemas": {
		"https://raw.githubusercontent.com/instrumenta/kubernetes-json-schema/master/v1.14.0-standalone-strict/all.json": ["/*.yaml"]
	},
	"yaml.enable": true,
	"yaml.schemaStore.enable": false,
	"yaml.schemaStore.url": "https://www.schemastore.org/api/json/catalog.json",
	"bashIde.includeAllWorkspaceSymbols": true,
	"bashIde.shellcheckPath": "/usr/bin/shellcheck",
	"bashIde.shellcheckArguments": "-f gcc",
	"sh.enable": true,
	"clangd.disableProgressNotifications": true,
	"clangd.enabled": true,
	"coc.source.around.priority": 0,
	"suggest.floatConfig": {},
	"coc.source.around.enable": true,
	"highlight.disableLanguages": [],
	"highlight.colors.enable": true,
	"colors.enable": true,
	"highlight.trace": "verbose",
	"pyright.enable": false,
	"python.jediEnabled": true,
	"python.autoUpdateLanguageServer": true,
	"python.autoComplete.addBrackets": true,
	"pyright.completion.snippetSupport": true,
	"http.proxy": "squid.corp.redhat.com:3128",
	"https.proxy": "squid.corp.redhat.com:3128",
	"https.proxyStrictSSL": false
}
