function! Preserve(command)
	let _s=@/
	let l = line(".")
	let c = col(".")
	execute a:command
	let @/=_s
	call cursor(l, c)
endfunction
" Ê†ºÂºèÂåñÂÖ®Êñá
function! FullFormat()
	call Preserve("normal gg=G")
endfunction

